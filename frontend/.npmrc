# Ensure npm uses the correct registry
registry=https://registry.npmjs.org/

# Disable audit for faster builds
audit=false

# Use legacy peer deps to avoid conflicts
legacy-peer-deps=true

# Force resolution of dependencies
force=true

# Use exact versions
save-exact=true

# Disable optional dependencies
optional=false

# Use npm cache
cache=.npm-cache

# Override dependency resolution
override-resolutions=true
